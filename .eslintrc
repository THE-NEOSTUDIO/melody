{
  "extends":"eslint:recommended",
  "ecmaFeatures":{
    "jsx":true,
    "modules":true
  },
  "env":{
    "browser":true,
    "node":true,
    "es6":true
  },
  "parser":"babel-eslint",
  "rules":{
    "strict":0,
    "valid-jsdoc":2,
    "react/jsx-uses-react":2,
    "react/jsx-uses-vars":2,
    "react/react-in-jsx-scope":2
  },
  "plugins":[
    "react"
  ]
}
